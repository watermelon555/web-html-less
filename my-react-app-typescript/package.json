{
  "name": "my-react-app-typescript",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://watermelon555.github.io/web-html-less",
  "dependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "@craco/craco": "^6.4.3",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.24",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "circular-dependency-plugin": "^5.2.2",
    "cra-template-typescript": "1.1.3",
    "craco-less": "^2.0.0",
    "less-plugin-npm-import": "^2.1.0",
    "mini-css-extract-plugin": "^2.5.2",
    "node-sass": "^7.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "^6.2.1",
    "react-router-dom": "^6.2.1",
    "react-scripts": "5.0.0",
    "sass-loader": "^12.4.0",
    "sass-resources-loader": "^2.2.4",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.5.4",
    "vconsole-webpack-plugin": "^1.7.3",
    "web-vitals": "^2.1.3",
    "webpack-bundle-analyzer": "^4.5.0"
  },
  "scripts": {
    "start": "set PORT=5000 && craco start",
    "build": "craco build",
    "test": "craco test",
    "lint": "tsc -p tsconfig.json --noEmit && eslint . --ext .jsx,.js,.ts,.tsx --fix && stylelint \"**/*.scss\" --syntax=scss --fix && prettier --write . --loglevel warn",
    "lint-staged": "lint-staged",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "standard"
    ],
    "parser": "@babel/eslint-parser"
  },
  "pre-commit": [
    "lint"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@types/react-router": "^5.1.17",
    "@types/react-router-dom": "^5.3.2",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "craco-sass-resources-loader": "^1.1.0",
    "eslint": "^8.13.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-standard": "^5.0.0",
    "font-awesome": "^4.7.0",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "pre-commit": "^1.2.2",
    "prettier": "^2.6.2",
    "prettier-eslint": "^13.0.0",
    "standard": "^16.0.4",
    "stylelint": "^14.6.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-recommended": "^7.0.0",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.5.0",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "ts-jest": "^27.1.2"
  },
  "lint-staged": {
    "**/*.{ts,tsx,scss}": [
      "prettier --write"
    ],
    "**/*.{ts,tsx}": "yarn lint",
    "**/*.scss": "stylelint --syntax scss"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged"
    }
  }
}
